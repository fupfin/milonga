<?xml version="1.0" encoding="UTF-8" ?>
<xsd:schema xmlns="http://www.skplanet.com/schema/milonga"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:beans="http://www.springframework.org/schema/beans"
	targetNamespace="http://www.skplanet.com/schema/milonga"
	elementFormDefault="qualified" attributeFormDefault="unqualified">
	
	<xsd:import namespace="http://www.springframework.org/schema/beans"/>
	
	<xsd:element name="milonga">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="location" minOccurs="1" maxOccurs="unbounded"/>
				<xsd:element name="autoRefreshable">
					<xsd:complexType>
						<xsd:attribute name="value" type="xsd:boolean"/>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
